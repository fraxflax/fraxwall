# ACCEPT|[N]REJECT|[N]DROP = \
#       <in-interface>	; <source-addr>	; <dest-addr>	; <protocol>	; <dest-port>

### WLAN
[_FW_]
  ACCEPT = IWLAN	; ALL		; ALL			; tcp		; squid
  ACCEPT = IWLAN        ; ALL           ; ALL                   ; tcp,udp       ; bootps

[PROXY]
  PROXY  = IWLAN	; ALL		; ALL			; tcp		; http \
			; localhost	; squid
  DIRECT = IWLAN	; ALL		; NLOCAL, NPRIV, \
					  NEXT, NEDMZs, \
					  NEIOIOLD \
								; tcp		; http
[IEXT]  
  NAT = 88.151.47.91 	; NWLAN		; ALL 		; ALL 		; ALL
  ACCEPT = IWLAN	; ALL		; ALL		; tcp		; https,http
 NREJECT = IWLAN	; ALL		; ALL		; tcp		; ALL
