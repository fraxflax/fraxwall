# ACCEPT|[N]REJECT|[N]DROP = \
#	<in-interface>	; <source-addr>	; <dest-addr>		; <protocol>	; <dest-port>

# petera: testing BSP at rasmusk's place
#[DEFINE]
#  ABSPTITLE01 = 10.116.13.130 # bsp-title01.ioi.dk (tcp/7000)
#  ABSPEXTTEST = NPRIV,213.114.10.92,212.242.127.40 # kia.axnet.nu - 213.114.10.92, rasmusk - 212.242.127.40

#[IBT]
#  ACCEPT = IEXT		; ABSPEXTTEST	; ABSPTITLE01	; tcp		; 7000
#  iptables = --table nat --insert PREROUTING --destination 88.151.47.91 --protocol tcp --destination-port 7000 \
#								--jump DNAT --to-destination 10.116.13.130:7000
[IEXT]
  NAT = 88.151.47.91 	; NBT		; ALL 			; ALL 		; ALL
  ACCEPT = IBT		; NBT		; ALL 			; tcp 		; http


[_FW_]
  ACCEPT = IBT		; NBT		; ALL			; tcp		; squid


[PROXY]
  PROXY  = IBT		; NBT		; ALL			; tcp		; http \
			; localhost	; squid
  DIRECT = IBT		; NBT		; NLOCAL, NPRIV, \
					  NEXT, NEDMZs, \
					  NEIOIOLD \
								; tcp		; http


