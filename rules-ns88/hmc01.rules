# ACCEPT = \
#       <in-interface>  ; <source-addr> ; <dest-addr>           ; <protocol>    ; <dest-port>
[IHMC]
  ACCEPT = IINTs	; ALL		; ALL			; ALL		; ALL
  ACCEPT = ALL		; ALL		; ALL			; tcp		; 1337

[IEXT]
  iptables = --table nat --insert PREROUTING --destination 88.151.47.139 --jump DNAT --to-destination 10.116.47.139
  NAT = 88.151.47.139	; 10.116.47.139 ; ALL 			; ALL 		; ALL


[IINT]
  ACCEPT = IHMC		; ALL		; hp_ntp
  ACCEPT = IHMC		; ALL		; hp_domain
