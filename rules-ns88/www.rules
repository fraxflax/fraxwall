# ACCEPT = \
#       <in-interface>	; <source-addr>	; <dest-addr>	; <protocol>	; <dest-port>
[IWWW]
  ACCEPT = ALL		; ALL		; ALL		; tcp		; http
  ACCEPT = ALL 		; ALL		; ALL		; icmp		; icmp_allow
  ACCEPT = IINTs	; ALL		; ALL		; tcp		; ssh, smtp

[IINT]
  ACCEPT = IWWW		; ALL		; hp_syscvs

[ISMTP]
  ACCEPT = IWWW		; ALL		; ALL		; tcp		; smtp # send mails via smtp.ioi.dk

[IEXT]
  ACCEPT = IWWW		; ALL		; ALL		; tcp		; smtp # job-announce-mailinglist

