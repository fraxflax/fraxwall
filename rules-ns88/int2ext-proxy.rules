# ACCEPT|[N]REJECT|[N]DROP = \
#       <in-interface>	; <source-addr>	; <dest-addr>		; <protocol>	; <dest-port>

# Setup web proxy
[_FW_]
  ACCEPT = IINTs	; ALL		; ALL			; tcp		; squid

[PROXY]
  PROXY  = IINTs	; ALL		; ALL			; tcp		; http \
			; localhost	; squid
  DIRECT = IINTs	; ALL		; NLOCAL, NPRIV, \
					  NEXT, NEDMZs, \
					  NEIOIOLD \
								; tcp		; http
  # Don't proxy viruswall
  DIRECT = IINT		; 172.21.17.17	; ALL			; tcp		; http, https #,8080
