#       <in-interface>	; <source-addr>		; <dest-addr>		; <protocol>	; <dest-port>

include = /etc/fraxwall/rules-ns88/gamecafe.defines

[IEXT]  
  NAT = 88.151.47.91 	; NGC			; ALL 			; ALL 		; ALL
  ACCEPT = IGC		; MGCP,MGCG,MPS23,MXBOX \
						; ALL			; ALL		; ALL
