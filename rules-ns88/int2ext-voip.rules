# ACCEPT|[N]REJECT|[N]DROP = \
#       <in-interface>	; <source-addr>	; <dest-addr>		; <protocol>	; <dest-port>

#[DEFINE]
#  APBX = 88.151.47.94
#
#[IEXT]
#  ACCEPT = IINTs	; ALL		; APBX			; ALL		; ALL

iptables = --table filter --insert usr01-ext --destination 88.151.47.94 --jump ACCEPT
iptables = --table filter --insert int-ext --destination 88.151.47.94 --jump ACCEPT
iptables = --table filter --insert tun-ext --destination 88.151.47.94 --jump ACCEPT
iptables = --table nat --insert ALL-ext --destination 88.151.47.94 --jump ACCEPT

